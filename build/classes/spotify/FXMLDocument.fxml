<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="482.0" prefWidth="482.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="spotify.FXMLDocumentController">
   <children>
      <MenuBar prefHeight="29.0" prefWidth="482.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#exitProgramActionPerformed" text="Close" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <ImageView fx:id="imageView" fitHeight="310.0" fitWidth="310.0" layoutX="86.0" layoutY="72.0" pickOnBounds="true" preserveRatio="true" />
      <Label alignment="CENTER" layoutX="86.0" layoutY="29.0" prefHeight="41.0" prefWidth="310.0" text="Spotify Album Covers">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <ProgressBar fx:id="bar" layoutX="5.0" layoutY="396.0" prefHeight="20.0" prefWidth="472.0" progress="0.0" />
      <Label layoutX="76.0" layoutY="416.0" prefHeight="29.0" prefWidth="48.0" text="Artist:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="artistTF" layoutX="127.0" layoutY="417.0" prefHeight="27.0" prefWidth="228.0" promptText="ex: Eminem" />
      <Button fx:id="previousButton" layoutX="8.0" layoutY="200.0" mnemonicParsing="false" onAction="#previousButton" prefHeight="27.0" prefWidth="75.0" text="Previous" />
      <Button fx:id="nextButton" layoutX="399.0" layoutY="200.0" mnemonicParsing="false" onAction="#nextButton" prefHeight="27.0" prefWidth="75.0" text="Next" />
      <Button fx:id="searchButton" layoutX="359.0" layoutY="417.0" mnemonicParsing="false" onAction="#searchButton" prefHeight="27.0" prefWidth="93.0" text="Search" />
      <Label fx:id="bottomMessage" alignment="CENTER" layoutX="151.0" layoutY="451.0" prefHeight="27.0" prefWidth="181.0" text="Cover">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="seeTracks" layoutX="359.0" layoutY="451.0" mnemonicParsing="false" onAction="#seeTracksActionPerformed" prefHeight="27.0" prefWidth="93.0" text="See Tracks" />
   </children>
</AnchorPane>
